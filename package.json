  "description": "TÃ¢mia Gatsby link componet",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/tamiadev/tamia-gatsby-link",
  "repository": "tamiadev/tamia-gatsby-link",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint . --cache --fix",
    "pretest": "npm run lint",
    "test": "true",
    "posttest": "npm run format",
    "format": "prettier --write '**/*.{js,md}'",
    "compile": "babel -d lib/ src/",
    "copy-flow": "flow-copy-source src/ lib/",
    "prepublishOnly": "npm run compile && npm run copy-flow",
    "precommit": "lint-staged"
  },
  "keywords": [
    "tamia"
  ],
  "dependencies": {},
  "peerDependencies": {
    "gatsby": "*",
    "tamia": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-flow": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-tamia": "^6.0.1",
    "flow-copy-source": "^2.0.2",
    "gatsby": "^2.0.0-beta.55",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "tamia": "^4.0.0-alpha.11"
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
